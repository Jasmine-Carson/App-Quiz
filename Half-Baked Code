<script>
	$(document).ready(function() {
		$("input").change(function(){		
			if ($("#checkbox-v-2a").is(":checked")) 
			{ $("#checkbox-v-2a-display").show();
			}
			else {$("#checkbox-v-2a-display").hide();
				}

			if ($("#checkbox-v-2b").is(":checked"))
			{ $("#checkbox-v-2b-display").show();
			}
			else {$("#checkbox-v-2b-display").hide();
			}
			
		    if ($("#checkbox-v-2c").is(":checked"))
			{ $("#checkbox-v-2c-display").show();
			}
			else{ $("#checkbox-v-2c-display").hide();
			}
	
			if ($("#checkbox-v-2d").is(":checked"))
			{ $("#checkbox-v-2d-display").show();
			}
			else{ $("#checkbox-v-2d-display").hide();
			}

			if ($("#checkbox-v-2e").is(":checked"))
			{ $("#checkbox-v-2e-display").show();
			}
			else{ $("#checkbox-v-2e-display").hide();
			}


			if ($("#checkbox-v-2f").is(":checked"))
			{ $("#checkbox-v-2f-display").show();
			}
			else{ $("#checkbox-v-2f-display").hide();
			}

			if ($("#checkbox-v-2g").is(":checked"))
			{ $("#checkbox-v-2g-display").show();
			}
			else{ $("#checkbox-v-2g-display").hide();
			}

			if ($("#checkbox-v-2h").is(":checked"))
			{ $("#checkbox-v-2h-display").show();
			}
			else{ $("#checkbox-v-2h-display").hide();
			};
		});
	});
</script>

